{
  "name": "onebun",
  "version": "0.1.0",
  "author": "RemRyahirev",
  "description": "OneBun framework monorepo - A bun.js framework inspired by nest.js with effect.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RemRyahirev/onebun.git"
  },
  "bugs": {
    "url": "https://github.com/RemRyahirev/onebun/issues"
  },
  "homepage": "https://github.com/RemRyahirev/onebun#readme",
  "engines": {
    "bun": "1.2.12",
    "node": "24"
  },
  "license": "LGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "bun run --watch example/src/index.ts",
    "dev:once": "bun run example/src/index.ts",
    "test": "bun test",
    "lint": "eslint --fix",
    "typecheck": "bunx tsc --noEmit",
    "publish:check": "bun scripts/publish.ts --dry-run",
    "publish:packages": "bun scripts/publish.ts",
    "version:bump": "bun scripts/version-bump.ts",
    "compose:up": "docker-compose -p onebun-example",
    "redis:cli": "docker-compose exec redis redis-cli -a $REDIS_PASSWORD",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "workspaces": [
    "packages/*",
    "example"
  ],
  "dependencies": {
    "effect": "^3.13.10"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "bun-types": "1.2.2",
    "eslint": "^9.30.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "testcontainers": "^11.7.1",
    "vitepress": "^1.6.4"
  }
}
